select l.aset_id,l.NilaiPerolehan,l.UmurEkonomis as UmurEkonomisSkrg,l2.UmurEkonomis as UmurEkonomisLama,(l.UmurEkonomis -l2.UmurEkonomis)as selisihUmur,l.AkumulasiPenyusutan as AkumulasiPenyusutanSkrg ,l2.AkumulasiPenyusutan as AkumulasiPenyusutanLama ,(l.AkumulasiPenyusutan-l2.AkumulasiPenyusutan)as selisihAkm,l.PenyusutanPerTahun as PenyusutanPerTahunSkrg ,l2.PenyusutanPerTahun as PenyusutanPerTahunLama,(l.PenyusutanPerTahun-l2.PenyusutanPerTahun) as SelisihPenyusutanPerTahun from log_bangunan l inner join log_bangunan l2 on l.Aset_Id=l2.Aset_ID and l2.kd_riwayat=49 and l2.tglperubahan='2016-12-31' where l.Kd_Riwayat=50 and l.tglperubahan='2016-12-31' and l2.UmurEkonomis!=0



select l.aset_id,l.NilaiPerolehan,l.UmurEkonomis as UmurEkonomisSkrg,l2.UmurEkonomis as UmurEkonomisLama,(l.UmurEkonomis -l2.UmurEkonomis)as selisihUmur,l.AkumulasiPenyusutan as AkumulasiPenyusutanSkrg ,l2.AkumulasiPenyusutan as AkumulasiPenyusutanLama ,(l.AkumulasiPenyusutan-l2.AkumulasiPenyusutan)as selisihAkm,l.PenyusutanPerTahun as PenyusutanPerTahunSkrg ,l2.PenyusutanPerTahun as PenyusutanPerTahunLama,(l.PenyusutanPerTahun-l2.PenyusutanPerTahun) as SelisihPenyusutanPerTahun from log_jaringan l inner join log_jaringan l2 on l.Aset_Id=l2.Aset_ID and l2.kd_riwayat=49 and l2.tglperubahan='2016-12-31' where l.Kd_Riwayat=50 and l.tglperubahan='2016-12-31' and l2.UmurEkonomis!=0


select l.aset_id,l.NilaiPerolehan,l.UmurEkonomis as UmurEkonomisSkrg,l2.UmurEkonomis as UmurEkonomisLama,(l.UmurEkonomis -l2.UmurEkonomis)as selisihUmur,l.AkumulasiPenyusutan as AkumulasiPenyusutanSkrg ,l2.AkumulasiPenyusutan as AkumulasiPenyusutanLama ,(l.AkumulasiPenyusutan-l2.AkumulasiPenyusutan)as selisihAkm,l.PenyusutanPerTahun as PenyusutanPerTahunSkrg ,l2.PenyusutanPerTahun as PenyusutanPerTahunLama,(l.PenyusutanPerTahun-l2.PenyusutanPerTahun) as SelisihPenyusutanPerTahun from log_mesin l inner join log_mesin l2 on l.Aset_Id=l2.Aset_ID and l2.kd_riwayat=49 and l2.tglperubahan='2016-12-31' where l.Kd_Riwayat=50 and l.tglperubahan='2016-12-31' and l2.UmurEkonomis!=0
