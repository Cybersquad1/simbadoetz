select l.aset_id,l.kodeSatker,s.NamaSatker,l.kodeKelompok,l.tahun,l.noregister,l.NilaiPerolehan as NilaiPerolehanAwal,l.AkumulasiPenyusutan,l.AkumulasiPenyusutan_Awal,(l.AkumulasiPenyusutan-l.AkumulasiPenyusutan_Awal)as koreksi,l.NilaiBuku_Awal as NilaiBukuSebelum,l.NilaiBuku as NilaiBukuSesudah from log_jaringan l inner join satker s on s.kode=l.kodeSatker and s.Kd_Ruang is null where kd_riwayat=55
